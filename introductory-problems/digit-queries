#include <iostream>
#include <vector>
#include <algorithm>
#include <string>
 
typedef long long ll;
 
void test_case()
{
	ll k{};
	std::cin >> k;
 
	ll count{ 9 };
	ll skipped{ 0 };
 
	for (ll len{ 1 }; true; ++len)
	{
		if (k > len * count)
		{
			k -= len * count;
			skipped += count;
		}
		else
		{
			ll skip{ (k - 1) / len };
			skipped += skip; 
 
			k -= skip * len; 
 
			ll num{ skipped + 1 };
			std::cout << std::to_string(num)[static_cast<std::size_t>(k - 1)] << "\n";
 
			return;
		}
 
		count *= 10; 
	}
}
 
int main()
{
	int q{};
	std::cin >> q; 
 
	for (int i{ 1 }; i <= q; ++i)
	{
		test_case();
	}
 
	return 0; 
 
}
