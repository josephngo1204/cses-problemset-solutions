#include <iostream>
#include <vector>
#include <algorithm>
 
int main()
{
	int n{};
	int m{};
	std::cin >> n >> m;
 
	std::vector<std::string> arr(static_cast<std::size_t>(n)); 
 
	for (std::string& c : arr)
	{
		std::cin >> c;
	}
 
	for (int row{ 0 }; row < n; ++row )
	{
		for (int col{ 0 }; col < m; ++col)
		{
			std::vector<bool> forb(4); 
 
			if (row != 0)
			{
				forb[static_cast<std::size_t>(arr[static_cast<std::size_t>(row - 1)][static_cast<std::size_t>(col)] - 'A')] = true;
			}
 
			if (col != 0)
			{
				forb[static_cast<std::size_t>(arr[static_cast<std::size_t>(row)][static_cast<std::size_t>(col - 1)] - 'A')] = true;
			}
 
			forb[static_cast<std::size_t>(arr[static_cast<std::size_t>(row)][static_cast<std::size_t>(col)] - 'A')] = true;
 
			for (int i{0}; i < forb.size(); ++i)
			{
				if (forb[static_cast<std::size_t>(i)] == false)
				{
					arr[static_cast<std::size_t>(row)][static_cast<std::size_t>(col)] = static_cast<char>(i + 'A');
					break; 
				}
			}
		}
 
		std::cout << arr[static_cast<std::size_t>(row)] << "\n"; 
	}
 
	return 0; 
 
}
