#include <vector>
#include <iostream>
 
int main()
{
	int n{}; 
	std::cin >> n; 
 
	if ((n == 2) || (n == 3))
	{
		std::cout << "NO SOLUTION";
		return 0; 
	}
 
	for (int i{ 2 }; i <= n; i += 2)
	{
		std::cout << i << " "; 
	}
 
	for (int j{ 1 }; j <= n; j += 2)
	{
		std::cout << j << " "; 
	}
 
	return 0; 
 
}
