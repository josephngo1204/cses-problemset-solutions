#include <iostream>
#include <vector>
#include <algorithm>
 
typedef long long ll;
 
int main()
{
	ll n{};
	ll x{}; 
	std::cin >> n >> x;
 
	std::vector<ll> num(static_cast<std::size_t>(x + 1)); 
	std::vector<ll> coins(static_cast<std::size_t>(n));
 
	for (ll i{ 0 }; i < n; ++i)
	{
		std::cin >> coins[static_cast<std::size_t>(i)];
	}
 
	num[0] = 0; 
	ll INF{ 10000000 }; 
 
	for (ll i{ 1 }; i <= x; ++i)
	{
		num[static_cast<std::size_t>(i)] = INF; 
 
		for (auto c : coins)
		{
			if (i - c >= 0)
			{
				num[static_cast<std::size_t>(i)] = std::min(num[static_cast<std::size_t>(i)], num[static_cast<std::size_t>(i - c)] + 1); 
			}
		}
	}
 
	if (num[static_cast<std::size_t>(x)] == INF)
	{
		std::cout << "-1"; 
		return 0; 
	}
 
	std::cout << num[static_cast<std::size_t>(x)]; 
 
	return 0; 
}
