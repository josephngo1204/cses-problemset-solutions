#include <iostream>
#include <vector>
#include <algorithm>
#include <functional>
 
typedef long long ll;
 
int main()
{
	ll n{};
	ll x{};
 
	std::cin >> n >> x;
 
	std::vector<std::pair<ll, ll>> arr(static_cast<std::size_t>(n));
 
	for (ll i{ 0 }; i < n; ++i)
	{
		ll a{};
		std::cin >> a;
 
		arr[static_cast<std::size_t>(i)] = { a, i + 1 };
	}
 
	std::sort(arr.begin(), arr.end()); 
 
	ll i{};
	ll j{ n - 1 };
 
	while (i < j)
	{
		if (arr[static_cast<std::size_t>(i)].first + arr[static_cast<std::size_t>(j)].first > x)
			--j;
 
		else if (arr[static_cast<std::size_t>(i)].first + arr[static_cast<std::size_t>(j)].first < x)
			++i;
 
		else if (arr[static_cast<std::size_t>(i)].first + arr[static_cast<std::size_t>(j)].first == x)
		{
			std::cout << arr[static_cast<std::size_t>(i)].second << " " << arr[static_cast<std::size_t>(j)].second; 
			return 0; 
		}	
	}
 
	std::cout << "IMPOSSIBLE"; 
 
	return 0; 
}
